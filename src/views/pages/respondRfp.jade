    <script id='title-placeHolder-tmpl' type="text/html">
        <% if(title){ %>
            <label class="text-capitalize font-md black" style="font-weight:normal;text-transform:uppercase"><%=title %></label>
        <% } %>
        
    </script>
    <script type="text/html" id="add-questions-tmpl" >
        <div>

          <!-- Nav tabs -->
          <ul class="nav nav-pills nav-tabs" role="tablist">
            <% _.each( questionGroups, function( questionItem , index){ %>
                <% if((userRole.indexOf('admin')>=0) || (userRole.indexOf(questionItem.title)>=0)){ %>
                    <% var count=0; %>
                     <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){
                                count++;
                               } 
                            %>
                    <% }); %>
                    <li role="presentation" class="<%=index==0?'active':'' %> col-sm-3 margin0 text-center">
                        <a href="<%='#question-'+index %>" aria-controls="<%='#question-'+index %>" role="tab" data-toggle="tab"><%=questionItem.title+' ('+count+')' %></a>
                    </li>
                <% } %>
            <% }); %>
        </ul>
        <div class="tab-content margin0">
          <% _.each( questionGroups, function( questionItem , index){ %>
            <div role="tabpanel" data-index="<%=index %>" data-gId='<%=questionItem._id %>' class="<%=index==0?'tab-pane active':'tab-pane' %>" id="<%='question-'+index %>">
                <div class='question-list '>
                    <% var count=0; %>
                     <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){
                                count++;
                               } 
                            %>
                    <% }); %>
                    <% if(count>0){ %>
                        <div>
                                       <!-- <div>
                                            <label class='bold black'>Total Questions <span class='question-count'><%=count %></span></label>
                                        </div>-->
                        </div>
                    <% } %>
                    <div>
                        <% var count=0; %>
                        <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){ %>
                                <div class='std-que' data-qId='<%=question._id %>' data-index='<%=i %>'>
                                    <div style="float:left; width:20px;">
                                        <label class='margin0'>
                                            <%=(++count) %>
                                        </label>
                                    </div>
                                    <div>
                                        <div>
                                            <label class="block black" name="<%='Q'+i %>"><%=question.title %></label>
                                        </div>
                                    </div>
                                    <div style='min-width:200px;clear:left;'>
                                        <% if(question.type=='0'){ %>
                                            <%  var val= _.find(answers,function(obj){
                                                    if(obj.qId==question._id){
                                                        return true;
                                                    }
                                                });
                                            val=val||{};
                                            %>
                                            <textarea type='text' placeholder='Answer' class='answer form-control' style="width: 95%;height:100px;resize:none;" data-type='0' value='<%=val.text %>' data-index='<% i %>'><%=val.text %></textarea>
                                        <% }else if(question.type=='1'){ %>
                                            <select class='answer' data-placeholder='Please select' data-type='1'  style='width:95%; border-radius:0;margin-left:20px;height:30px;'>
                                               <option></option>
                                               <%   var val1= _.find(answers,function(obj){
                                                        if(obj.qId==question._id){
                                                            return true;
                                                        }
                                                    });
                                                    val1=val1||{};
                                                %>
                                               <% _.each( question.answerEnum, function( opt , index){ 
                                                    if(_.indexOf(val1.enumAns, opt)){
                                                        %>
                                                        <option  value='<%=opt %>'><%=opt %></option>
                                                        <%
                                                    }else{
                                                        %>
                                                        <option selected value='<%=opt %>'><%=opt %></option>
                                                        <%
                                                    }%>
                                                <% }); %>
                                            </select>
                                        <% }else{ %>
                                            <%   var val2= _.find(answers,function(obj){
                                                        if(obj.qId==question._id){
                                                            return true;
                                                        }
                                                    });
                                                    val2=val2||{};
                                            %>
                                            <select class='answer' data-placeholder='Please select' data-type='2'  multiple style='width:95%; border-radius:0;margin-left:20px;height:30px;'>
                                               <% _.each( question.answerEnum, function( opt , index){
                                                    if(_.indexOf(val2.enumAns, opt)>=0){
                                                        %>
                                                        <option selected value='<%=opt %>'><%=opt %></option>
                                                        <%
                                                    }else{
                                                        %>
                                                        <option  value='<%=opt %>'><%=opt %></option>
                                                        <%
                                                    }%>
                                                <% }); %>
                                            </select>
                                        <% } %>
                                    </div>
                                    <div style="clear:left;">
                                            <%  var val3= _.find(answers,function(obj){
                                                    if(obj.qId==question._id){
                                                        return true;
                                                    }
                                                });
                                            val3=val3||{comment:[{},]};
                                            val3.comment=val3.comment||[{},];
                                            val3.comment[0]=val3.comment[0]||{};
                                            %>
                                            <div class='commentContainer' style='margin-top:10px;'>
                                                <a href='#' class='commentToggle' style='margin: 10px 0 10px 20px;'>add comment</a>
                                                <div class='commentSection hidden'>
                                                    <textarea placeholder='Comment' class='comment form-control' style='margin: 10px 0 10px 20px;width: 95%;'><%=val3.comment[0].text %></textarea>
                                                </div>
                                            </div>
                                            <!--<textarea class='comment form-control' placeholder='comment' style="margin: 10px 0 10px 20px;width: 95%;"><%=val3.comment[0].text %></textarea>-->
                                    </div>
                                    
                                </div>
                            <% } %>
                        <% }); %>
                    </div>
                </div>
            </div>
          <% }); %>
        </div>
    </script>

    <script type="text/html" id="choose-answers-tmpl" >
        <div>

          <!-- Nav tabs -->
          <ul class="nav nav-pills nav-tabs" role="tablist">
            <% _.each( questionGroups, function( questionItem , index){ %>
                <% var count=0; %>
                     <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){
                                count++;
                               } 
                            %>
                    <% }); %>
                <li role="presentation" class="<%=index==0?'active':'' %> col-sm-3 margin0 text-center">
