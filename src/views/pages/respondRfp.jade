    <script id='add-contrib-tmpl'  type="text/html">
        <div class="panel panel-default"><div class="panel-body padding0">
        <div class="select_contributor">
            <label>Select Contributor</label>
            <div>
                <select data-placeholder='Contributor Name' data-master="people" name='name' style='width:40%;margin:0;min-height:30px;'></select>
                <input type='text' placeholder='Email' name='email' style="width:40%;min-height:40px; border-radius:5px; border:1px solid #ccc;">
                <% if(userRole.indexOf('admin')>=0){ %>
                    <input type='checkbox' name='role' value='admin' >Admin
                <% } %>
            </div>
            <div>
                <% if((userRole.indexOf('Functional')>=0) || (userRole.indexOf('admin')>=0)){ %>
                    <input type='checkbox' name='role' value='Functional'><span class="padding_50">Functional</span>
                <% } %>
                <% if((userRole.indexOf('Business')>=0) || (userRole.indexOf('admin')>=0)){ %>
                    <input type='checkbox' name='role' value='Business'><span class="padding_50">Business</span>
                <% } %>
                <% if((userRole.indexOf('Technical')>=0) || (userRole.indexOf('admin')>=0)){ %>
                    <input type='checkbox' name='role' value='Technical'><span class="padding_50">Technical</span>
                <% } %>
                <% if((userRole.indexOf('Legal & Contract')>=0) || (userRole.indexOf('admin')>=0)){ %>
                    <input type='checkbox' name='role' value='Legal & Contract'><span class="padding_50">Legal & Contract</span>
                <% } %>
            </div>
            <div class='error-container'></div>
            <div>
                <button id='save-contrib' class='btn btn-primary'>Save<i class="fa fa-floppy-o"></i></button>
                <button id='cancle-contrib' class='btn'>Cancel</button>
            </div>
        </div>
        <div>
            <label class="added">Added Contributors</label>
        </div>
        <table class='table table-responsive table-striped'>
            <% _.each(rederContrib,function(contrib,indx){ %>
                <tr>
                    <td><%=indx+1 %></td>
                    <td><%=contrib.name+' '+contrib.lastName %></td>
                    <td><%=contrib.email %></td>
                    <% if(contrib.role.indexOf('admin')>=0){ %>
                        <td>Admin</td>
                    <% } else{%>
                        <td><%=contrib.role %></td>
                    <% }%>
                </tr>
            <% }) %>

        </table>
        </div>
    </script>
