                                <label class='bold black'>Total Questions <span class='question-count'><%=count %></span></label>
                            </div>-->
                        </div>
                    <% } %>
                    <div>
                        <% var count=0; %>
                        <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){ %>
                              
                                <div class='std-que answer-choice' data-qId='<%=question._id %>' data-index='<%=i %>'>
                                    <div style="float: left;width: 20px;">
                                        <label class='margin0'>
                                            <%=(++count) %>
                                        </label>
                                    </div>
                                    <div>
                                        <div>
                                            <label class="block black" name="<%='Q'+i %>"><%=question.title %></label>
                                        </div>
                                    </div>
                                    <select class='notSelect2 chooseAnswer'  style="clear: left;display: block;margin:0 20px; width: 40%;padding: 5px 10px;border: 1px solid #ccc;border-radius: 5px;">
                                      <% _.each( responseList, function( response , inx){  %>
                                        <option data-isSelected='<%=response.userId.isSelected %>' value='<%=response.userId._id %>'><%=response.userId.name+' '+response.userId.lastName+"'s Answer" %>
                                        </option>
                                      <% }); %>
                                    </select>
                                  <% _.each( responseList, function( response , ix){ 
                                    var isAnswered=false;
                                  %>
                                  <div class='eachAnswer' data-user='<%=response.userId._id %>' style="margin-left: 20px; display:none;">
                                    <div  style='min-width:200px'>
                                        <% if(question.type=='0'){ %>
                                            <%  var val= _.find(response.answers,function(obj){
                                                    if(obj.qId==question._id){
                                                        return true;
                                                    }
                                                });
                                           
                                            if(val){
                                                isAnswered=true;
                                            }
                                            val=val||{};
                                            if(isAnswered){
                                            %>
                                            <label class='answer' style='margin: 5px 0; font-weight:normal;' data-type='0'  data-index='<% i %>'><%=val.text || 'No answer provided' %></label>
                                            <% } else{%>
                                            <label class='answer' style='margin: 5px 0; font-weight:normal;' data-type='0'  data-index='<% i %>'><%='No answer provided' %></label>
                                        <% }}else if(question.type=='1'){ %>
                                            
                                               <%   var val1= _.find(response.answers,function(obj){
                                                        if(obj.qId==question._id){
                                                            return true;
                                                        }
                                                    });
                                                    if(val1){
                                                        isAnswered=true;
                                                    }
                                                    val1=val1||{};
                                                    if(isAnswered){
                                                %>
                                                        <label  class='answer' style='margin: 5px 0; font-weight:normal;'><%=val1.enumAns %></label>
                                                    <% } else{%>
                                                        <label class='answer' style='margin: 5px 0; font-weight:normal;' data-type='1'   data-index='<% i %>'><%='No answer provided' %></label>
                                                    <% } %>
                                        <% }else{ %>
                                            <%   var val2= _.find(response.answers,function(obj){
                                                        if(obj.qId==question._id){
                                                            return true;
                                                        }
                                                    });
                                                    
                                                    
                                                    if(val2){
                                                        isAnswered=true;
                                                    }
                                                   val2=val2||{};
                                                    if(isAnswered){
                                            %>
                                                        <label class='answer' style='margin: 5px 0; font-weight:normal;'><%=val2.enumAns %>.</label>
                                                    <% } else{%>
                                                        <label class='answer' style='margin: 5px 0; font-weight:normal;' data-type='2'   data-index='<% i %>'><%='No answer provided' %></label>
                                                    <% } %>
                                        <% } %>
                                    </div>
                                    <div>
                                            <%  var val3= _.find(response.answers,function(obj){
                                                    if(obj.qId==question._id){
                                                        return true;
                                                    }
                                                });
                                            val3=val3||{comment:[{},]};
                                            val3.comment=val3.comment||[{},];
                                            val3.comment[0]=val3.comment[0]||{};
                                            %>
                                            <% if(isAnswered && val3.comment[0].text){
                                            %>
                                            <a href='#' class='commentToggle'>show comments</a>
                                            <div class='commentSection hidden'>
                                                <textarea disabled class='comment form-control' style='margin: 5px 0;'><%=val3.comment[0].text %></textarea>
                                            </div>
                                            <% } %>
                                    </div>
                                  </div>
                                  <% }); %>
                                </div>
                            <% } %>
                        <% }); %>
                    </div>
                </div>
            </div>
          <% }); %>
        </div>
    </script>
