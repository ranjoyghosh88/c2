                <% if(rfpProfile.solutionsInPlaceIds){ 
                    for(var indx=0;indx<rfpProfile.solutionsInPlaceIds.length;indx++){
                %>
                  <div style='margin: 5px 0;text-transform: capitalize;'><%=rfpProfile.solutionsInPlaceIds[indx].name %></div>
                <% } 
                  }
                %>
                <% if(rfpProfile.whyRFP){ %>
                  <div style='margin: 5px 0;text-transform: capitalize;'><%=rfpProfile.whyRFP %></div>
                <% } %>
                <% if(rfpProfile.productList && rfpProfile.productList.length>0){ %>
                  <label style='margin: 5px 0;text-transform: capitalize;'>Product to consider</label>
                    <% _.each(rfpProfile.productList,function(prod,ind){%>

