    <script type="text/html" id="rfp-input-template">
        <div class="rfp-select">
            <label class="block"><%= title%></label>
            <% for(var iIndex=0;iIndex< inputs.length;iIndex++) { 
              var data = inputs[iIndex];
            %>
            <div class="inline-block">
                <% if(data.disabled){ %>
                    <input type="<%= type%>" name="<%= name%>" value="<%= data.value%>" disabled/>
                <% }else{ %>
                    <input type="<%= type%>" name="<%= name%>" value="<%= data.value%>" class="margin0" />
                <% } %>
                <span><%= data.text%> </span>
            </div>  
            <%}%>
        </div>
    </script>
    <script type="text/html" id="title-text">
        <div class="title-text">
            <label><%= title%></label></br>
            <input type="text" class='form-control' name="<%= name%>" placeholder="Please Enter Title" value="<%= value || ''%>" />
        </div>
    </script>
    <script type="text/html" id="add-company">
        <div>
            <a href='#'  class='btn btn-link' data-toggle="<%= toggle%>" data-target="<%= target%>">Add company</a>
        </div>
    </script>
    <script type="text/html" id="inquire-product-tmpl">
        <div class="radio-arrange">
            <label class="block">Do you know which product you would like to inquire about?</label>
            <div class="inline-block">
                <input type="radio" name="hasProduct" value="Yes"  checked="checked"/>
                <span>Yes</span>
            </div>
            <div class="inline-block">
                <input type="radio" name="hasProduct" value="No" disabled/>
                <span>No</span>
            </div>
            <div>
                <select class="form-control" name="select-product" data-master="products" data-placeholder='Select Product' style="width: 100%" multiple="multiple"></select>
            </div>
        </div>
        <div>
            <label class="block">There are multiple companies that can help you with these products , please select the ones you are interested in having Respond</label>
            <select class="form-control" name="select-intrested-company" data-master="companies" data-placeholder='Select Company' style="width: 100%" multiple="multiple"></select>
        </div>
        <div class="radio-arrange">
            <label class="block">1a. Would you like a needs analysis to help determine the best potential options?</label>
            <div class="inline-block">
                <input type="radio" name="analysis-help" value="Yes"  checked="checked"/>
                <label>Yes</label>
            </div>
            <div class="inline-block"> 
                <input type="radio" name="analysis-help" value="No" />
                <label>No</label>
            </div>
        </div>
        <div>
            <label class="block">1b. What type of product type (ie ERP) or Business Process Areas (Accounting are you trying find a solution for?</label>
            <select class="form-control" name="type-of-prod" data-master="developmentSoftware" data-placeholder='product type or Business Process Areas ' style="width: 100%" multiple="multiple"></select>
        </div>
    </script>
    <script type="text/html" id="rfp-product-q-1">
        <div>
            <label class="block" style='margin-top:20px'><%= title%></label>
            <select class="form-control" name="<%= name%>" data-placeholder='Select <%=placeholder %>' style="width: 100%">
                <option></option>
                <% for(var iIndex=0;iIndex< options.length;iIndex++) { 
                    var opt = options[iIndex];
                    if(typeof(opt)!=='object'){
                       opt._id = opt;
                       opt.name = opt
                      }
                %>
                <option value='<%= opt._id%>'><%= opt.name%></option>
                <%}%>
            </select>
        </div>
    </script>
    <script type="text/html" id="select-typeahead" >
        <div class="select-company">
