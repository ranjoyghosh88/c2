                  <label style='margin: 5px 0;text-transform: capitalize;'><%=rfpProfile.title %></label>
                <% } %>
                <% if(rfpProfile.companyId){ %>
                  <div style='margin: 5px 0;text-transform: capitalize;'>For <label><%=rfpProfile.companyId.name %><label></div>
                <% } %>
                <% if(rfpProfile.timeFrame){ %>
                  <div style='margin: 5px 0;text-transform: capitalize;'><%=rfpProfile.timeFrame %></div>
                <% } %>
                <% if(rfpProfile.budget){ %>
                  <div style='margin: 5px 0;text-transform: capitalize;'><%=rfpProfile.budget %> purchase</div>
