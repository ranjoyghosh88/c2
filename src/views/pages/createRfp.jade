    <script type="text/html" id="add-questions-tmpl" >
        <div>

          <!-- Nav tabs -->
          <ul class="nav nav-pills nav-tabs" role="tablist">
            <% _.each( questionGroups, function( questionItem , index){ %>
                <% var count=0; %>
                     <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){
                                count++;
                               } 
                            %>
                    <% }); %>
                <li role="presentation" class="<%=index==0?'active':'' %> col-sm-3 margin0 text-center">
                    <a href="<%='#question-'+index %>" aria-controls="<%='#question-'+index %>" role="tab" data-toggle="tab"><%=questionItem.title+' ('+count+')' %></a>
                </li>
          
            <% }); %>
        </ul>
        <div class="tab-content margin0">
          <% _.each( questionGroups, function( questionItem , index){ %>
            <div role="tabpanel" data-index="<%=index %>" class="<%=index==0?'tab-pane active':'tab-pane' %>" id="<%='question-'+index %>">
                <div class='add-std-que-container radio-arrange'>
                    <label class="">Would you like to utilize our standard questions for an ERP RFP?</label>
                    <div class="inline-block">
                        <input type="radio" data-index="<%=index %>" 
                        <% if(questionItem.isStdQuestion) {%>
                                checked=checked
                        <%}%>

                          class='add-std-que' name="<%='add-std-func-que-'+index %>" value="Yes"/>
                        <span>Yes</span>
                    </div>
                    <div class="inline-block"> 
                        <input type="radio" data-index="<%=index %>" 
                        <% if(!questionItem.isStdQuestion) {%>
                                checked=checked
                        <%}%>
                        class='add-std-que' name="<%='add-std-func-que-'+index %>" value="No" />
                        <span>No</span>
                    </div>
                </div>
                <table class='question-list table table-bordered'>
                    <% var count=0; %>
                     <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){
                                count++;
                               } 
                            %>
                    <% }); %>
                    <% if(count>0){ %>
                        <thead>
                                        <th colspan='3'>
                                            <!--<label class=' black'>Total Questions <span class='question-count'><%=count %></span></label>-->
                                        </th>
                                        <th class='text-center'>
                                            <label class=' black'>Critical Value</label>
                                        </th>
                                        <th style='min-width:100px'  class='text-center'>
                                            <label class=' black'>Scored</label>
                                        </th>
                                        <th style='min-width:100px'  class='text-center'>
                                            <label class=' black'>Weight</label>
                                        </th>
                        </thead>
                    <% } %>
                    <tbody>
                        <% var count=0; %>
                        <% _.each( questionItem.questions, function( question , i){ %>
                            <% if( (questionItem.isStdQuestion && question.isStandard) || (!question.isStandard) ){ %>
                                <tr class='std-que' data-index='<%=i %>'>
                                    <td>
                                        <label class=''>
                                            <%=(++count) %>
                                        </label>
                                    </td>
                                    <td>
                                        <div>
                                            <label class="block black" name="<%='Q'+i %>"><%=question.title %></label>
                                        </div>
                                        
                                        <% if(question.subGroup){ %>
                                            <div>
                                                <label class='font-sm'>subgroup</label>
                                                <label class='font-sm'><%=question.subGroup %></label>
                                            </div>
                                        <% } %>
                                        <div>
                                            <label class='font-sm'><%=questionTypes[question.type] %></label>
                                        </div>
                                    </td>
                                    <td  class='text-center hover-primary' style='min-width: 60px;padding: 25px 0;'>
                                            <button title='Edit Question' class='btn btn-link fa fa-pencil-square-o edit-que black ' data-index='<%=i %>' style='padding:0;'></button>
                                            <button title='Delete Question' class='btn btn-link fa fa-trash-o delete-que black ' data-index='<%=i %>' style='padding:0;'></button>
                                    </td>
                                        <% if(question.critical){ %>
                                            <td class='text-center' style='min-width:200px'>
                                               
                                                <select data-placeholder='Please select critical value' class='critical-select' style='width:95%; border-radius:0;'>
                                                   <% _.each( question.answerEnum, function( opt , index){ 
                                                    if(_.contains(question.criticalValue, opt)){
                                                    %>
                                                        <option value='<%=opt %>' selected><%=opt %></option>
                                                    <% }else{ %>
                                                         <option value='<%=opt %>' ><%=opt %></option>
                                                    <% } %>
                                                    <% }); %>
                                                </select>
                                            </td>
                                            <td class='text-center'>
                                                <input type='checkbox' class='score-select' />
                                            </td>
                                            <td class='text-center'>
                                                <input type='hidden' disabled class='weight' value='<%=question.weight %>'/>
                                            </td>
                                        <% }else{ %>
                                                <% if(question.type=='0'){ %>
                                                    <td class='text-center' style='min-width:200px'>
                                                       
                                                        <select  data-placeholder='Please select critical value' class='critical-select' disabled style='width:95%; border-radius:0;'>
                                                           <option></option>
                                                           <% _.each( question.answerEnum, function( opt , index){ 
                                                                if(_.contains(question.criticalValue, opt)){
                                                                %>
                                                                    <option value='<%=opt %>'><%=opt %></option>
                                                                <% }else{ %>
                                                                     <option value='<%=opt %>' ><%=opt %></option>
                                                                <% } %>
                                                            <% }); %>
                                                        </select>
                                                    </td>
                                                <% }else{ %>
                                                    <td class='text-center' style='min-width:200px'>
                                                       
                                                        <select  data-placeholder='Please select critical value' class='critical-select' style='width:95%; border-radius:0;'>
                                                           <option></option>
                                                           <% _.each( question.answerEnum, function( opt , index){ 
                                                                if(_.contains(question.criticalValue, opt)){
                                                                %>
                                                                    <option value='<%=opt %>'><%=opt %></option>
                                                                <% }else{ %>
                                                                     <option value='<%=opt %>' ><%=opt %></option>
                                                                <% } %>
                                                            <% }); %>
                                                        </select>
                                                    </td>
                                                <%} %>
                                            <td class='text-center'>
                                                <input type='checkbox' class='score-select' checked="checked"/>
                                            </td>
                                            <td class='text-center'>
                                                <input type='hidden' class='weight' value='<%=question.weight %>'/>
                                            </td>
                                        <%} %>
                                    
                                </tr>
                            <% } %>
                        <% }); %>
                    <tbody>
                </table>
            <form class='addNewQueContainer'></form>
            <div class='row border-top'>
                <div class='col-xs-12'>
                    <button class='btn btn-primary add-que text-uppercase font-secondary marginT_30'>Add Question <i class="fa fa-plus"></i></button>
                </div>
            </div>
            </div>
          <% }); %>
        </div>
    </script>
