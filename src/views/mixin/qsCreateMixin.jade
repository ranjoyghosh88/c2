mixin createProductModel()
	#createProductPopup.modal.fade(role='dialog', backdrop: 'static',
    keyboard: false)
		.modal-dialog
			.modal-content
				.modal-header
					button.close.text-inverse(type='button', data-dismiss='modal') &times;    
					h4.modal-title.text-inverse.text-uppercase.font-secondary Create new product
				.modal-body
					form.form-horizontal#product-create-form
						.row
							.col-sm-3.col-xs-12.text-left.tableDisplay
								label.widthadjust.middleAlign CSquire Service
							.col-sm-9.col-xs-12
								select.form-control.pull-right(name='csquireService',id='csquireService', data-placeholder='Select CSquire Service',data-errorholder='' onchange='populateAttr(this)')
									if csquireServices && csquireServices.length
										option(value='') Select Csquire Service
										each item in csquireServices
											option(value=item._id)= item.name
								label.error.alert.alert-danger.marginLabel(style="display:none") Please Select CSquire Service
						.row.filterMargin
							.col-sm-3.col-xs-12.text-left.tableDisplay
								label.widthadjust.middleAlign Product Name
							.col-sm-9.col-xs-12
								input.form-control(name='productName',id='productName',placeholder="Enter product name",type='text' onkeyup="ValidateProductField(this)")
								label.error.alert.alert-danger.marginLabel(style="display:none") Please enter Product Name
								label.error.alert.alert-danger.marginLabel(style="display:none") Special character not allowed
								label.error.alert.alert-danger.marginLabel(style="display:none") Product Already exists
						.row.filterMargin
							.col-sm-3.col-xs-12.text-left.tableDisplay
								label.widthadjust.middleAlign Attributes
							.col-sm-9.col-xs-12
								input.form-control(id="attrValue",placeholder="To, From",type='text', name='attrValue', readonly='readonly')
						.row.filterMargin
							.col-sm-3.col-xs-12.text-left
								label.widthadjust(style="margin-top:10px") Description
							.col-sm-9.col-xs-12
								textarea.textarea_description.form-control(name='productDesc',id='productDesc',placeholder="Enter description of product" onkeyup="removeAlert(this)")
								label.error.alert.alert-danger.marginLabel(style="display:none") Please enter description
				.modal-footer
					button.btn.btn-default.btn-primary.btn-heading-small(type='button', id = "addStripeProductCreate") Create
					button.btn.btn-default.border-primary.btn-heading-small(type='button', id = "addStripeProductReset") Clear All
