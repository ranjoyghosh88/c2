mixin createCouponModel()
	#createCouponPopup.modal.fade(role='dialog', backdrop: 'static',
    keyboard: false)
		.modal-dialog
			.modal-content
				.modal-header
					button.close.text-inverse(type='button', data-dismiss='modal') &times;    
					h4.modal-title.text-inverse.text-uppercase.font-secondary create coupon
				.modal-body
					form.form-horizontal.signupForm#coupon-create-form
						.row
							.col-sm-3.col-xs-12.text-left.tableDisplay
								label.widthadjust.middleAlign Coupon ID
							.col-sm-9.col-xs-12
								input.form-control(name='createCoupon',id='createCouponId',placeholder="Enter Coupon ID" onkeyup="ValidateField(this)")
								label.error.alert.alert-danger.marginLabel(style="display:none") Please enter Coupon ID
								label.error.alert.alert-danger.marginLabel(style="display:none") Special character not allowed
								label.error.alert.alert-danger.marginLabel(style="display:none") Coupon Already exists
						.row.filterMargin
							.col-sm-3.col-xs-12.text-left.tableDisplay
								label.widthadjust.middleAlign Value
							.col-sm-3.col-xs-4
								select.form-control.pull-right(name='offMode',id='offMode',data-placeholder='$', onChange='showMsgBox()')
									option(value='$' selected) $
									option(value='%') %
							.col-sm-6.col-xs-8
								input.form-control(placeholder="Enter Value",name="value",id="value",onkeyup="removeAlert(this)")
								label.error.alert.alert-danger.marginLabel(style="display:none") Please enter Value
								label.error.alert.alert-danger.marginLabel(style="display:none") This will be applicable on total value
								label.error.alert.alert-danger.marginLabel(style="display:none") Must be between 1 and 100
						.row.filterMargin
							.col-sm-3.col-xs-12.text-left.tableDisplay
								label.widthadjust.middleAlign Redeem By
							.col-sm-9.col-xs-12#redeemByDatePick.relative
								input#redeemBy.form-control.clsDatePicker(placeholder="Select redemeption date",type='text', name='redeemBy', readonly='readonly')
								span.absolute.glyphicon.glyphicon-calendar
						.row.filterMargin
							.col-sm-3.col-xs-12.text-left.tableDisplay
								label.widthadjust.middleAlign Max Redemptions
							.col-sm-9.col-xs-12
								input.form-control(placeholder="Count of redemption",name="maxRedemption",onkeyup="ValidateFieldNum(this)")
								input(type='hidden' name='duration', value = "once")
				.modal-footer
					button.btn.btn-default.btn-primary.btn-heading-small(type='button', id = "addStripeCouponCreate") Create
					button.btn.btn-default.border-primary.btn-heading-small(type='reset', id = "addStripeCouponReset") Clear All
